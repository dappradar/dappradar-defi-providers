    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug npm test with arguments",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "test",
                "--",
                "base",
                "uniswapv3",
                "16905122"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "TS_NODE_TRANSPILE_ONLY": "true"
            }
        }
    ]
}